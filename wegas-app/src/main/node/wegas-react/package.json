{
  "name": "wegas_react",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean": "rimraf ../../../../target/Wegas/wegas-react",
    "start": "webpack serve --hot --mode development",
    "start-no-unused": "cross-env TS_NODE_PROJECT=\"tsconfig-no-unused.json\" webpack serve --hot --mode development",
    "preprod": "cross-env NODE_ENV=pre-production  webpack serve --hot --mode production",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "test": "jest",
    "lint": "eslint 'src' --ext '.js,.ts,.tsx'",
    "stats": "rimraf dist && cross-env NODE_ENV=stats webpack --mode production"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "lodash-es": "lodash"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": false,
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },

{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "jsRules": {
        "quotemark": [
            true,
            "single",
            "jsx-double",
            "avoid-escape"
        ],
        "object-literal-sort-keys": false
    },
    "rules": {
        "quotemark": [
            true,
            "single",
            "jsx-double",
            "avoid-escape"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": {
            "severity": "warning"
        },
        "member-access": false,
        "array-type": [
            true,
            "array"
        ],
        "jsx-wrap-multiline": false,
        "jsx-boolean-value": [
            true,
            "never"
        ]
    },
    "rulesDirectory": []
}
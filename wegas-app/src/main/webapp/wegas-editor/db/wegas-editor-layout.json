{
    "id": 2003,
    "type" : "Layout",
    "top" : {
        "height": 25,
        "scroll": null,
        "zIndex": 2,
        "children": [{
            "type": "Button",
            "content": "",
            "cssClass": "wegas-title",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "Button",
                        "label": "Properties",
                        "plugins": [{
                            "fn": "EditEntityAction",
                            "cfg": {
                                "entity": "currentGameModel",
                                "dataSource": "GameModelFacade"
                            }
                        }]
                    }, {
                        "type": "Button",
                        "label": "Duplicate",
                        "plugins": [{
                            "fn": "DuplicateEntityAction",
                            "cfg": {
                                "entity": "currentGameModel",
                                "dataSource": "GameModelFacade"
                            }
                        }]
                    }, {
                        "type": "DeleteEntityButton",
                        "cfg": {
                            "entity": "currentGameModel",
                            "dataSource": "GameModelFacade"
                        }
                    }]
                }
            }]
        }, {
            "type": "SelectGameButton"
        }, {
            "type": "SelectPlayerButton"
        }, {
            "type": "Button",
            "label": "Developer",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "OpenTabButton",
                        "label": "Console",
                        "children": {
                            "type": "Console",
                            "class": "wegas-console",
                            "plugins": [{
                                "fn": "WidgetToolbar",
                                "cfg": {
                                    "children": [{
                                        "type": "SelectPlayerButton"
                                    }]
                                }
                            }]
                        }
                    }]
                }
            }]
        },{
            "type": "Button",
            "label": "Administrator",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "OpenTabButton",
                        "label": "Users",
                        "children": {
                            "type": "EditorTreeView",
                            "dataSource": "User",
                            "plugins": [{
                                "fn": "WidgetToolbar",
                                "cfg": {
                                    "children": [{
                                        "type": "NewEntityButton",
                                        "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                        "targetClass": "JpaAccount",
                                        "dataSource": "User"
                                    }]
                                }
                            }]
                        }
                    }, {
                        "type": "OpenTabButton",
                        "label": "Groups",
                        "children": {
                            "type": "EditorTreeView",
                            "dataSource": "Role",
                            "plugins": [{
                                "fn": "WidgetToolbar",
                                "cfg": {
                                    "children": [{
                                        "type": "NewEntityButton",
                                        "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                        "targetClass": "Role",
                                        "dataSource": "Role"
                                    }]
                                }
                            }]
                        }
                    }]
                }
            }]
        },{
            "type": "Button",
            "label": "Scenarist",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "OpenTabButton",
                        "label": "Styles",
                        "children": {
                            "type": "CSSEditor",
                            "class": "wegas-csseditor"
                        }
                    },{
                        "type": "OpenTabButton",
                        "label": "Files",
                        "children": {
                            "type": "FileExplorer",
                            "plugins": [{
                                "fn": "WidgetToolbar"
                            }]
                        }
                    },{
                        "type": "OpenTabButton",
                        "label": "Scripts",
                        "children": {
                            "type": "ScriptLibrary"
                        }
                    }, {
                        "type": "OpenTabButton",
                        "label": "Pages",
                        "tabSelector": "#leftTabView",
                        "children": {
                            "type": "PageTreeview",
                            "pageLoader": "previewPageLoader"
                        }
                    }]
                }
            }]
        },{
            "type": "Button",
            "label": "Animator",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "OpenTabButton",
                        "label": "Players",
                        "children": {
                            "type": "EditorTreeView",
                            "dataSource": "Game"
                        }
                    }]
                }
            }]
        },{
            "type": "LoginButton",
            "cssClass": "menu-loginbutton"
        }]
    },
    "left": {
        "width": 300,
        "resize": true,
        "scroll": true,
        "animate": true,
        "children": [{
            "type": "TabView",
            "id": "leftTabView",
            "children": [{
                "type": "Tab",
                "label": "Variables",
                "children": [{
                    "type": "VariableDescriptorTreeview",
                    "dataSource": "VariableDescriptor",
                    "excludeClasses": {
                        "MCQDescriptor": true
                    },
                    "plugins": [{
                        "fn": "WidgetToolbar",
                        "cfg": {
                            "children": [{
                                "type": "Button",
                                "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                "plugins": [{
                                    "fn": "WidgetMenu",
                                    "cfg": {
                                        "children":  [{
                                            "type": "NewEntityButton",
                                            "label": "Number",
                                            "targetClass": "NumberDescriptor"
                                        }, {
                                            "type": "NewEntityButton",
                                            "label": "String",
                                            "targetClass": "StringDescriptor"
                                        }, {
                                            "type": "NewEntityButton",
                                            "label": "List",
                                            "targetClass": "ListDescriptor"
                                        }, {
                                            "type": "NewEntityButton",
                                            "label": "Question",
                                            "targetClass": "QuestionDescriptor"
                                        }, {
                                            "type": "NewEntityButton",
                                            "label": "Trigger",
                                            "targetClass": "TriggerDescriptor"
                                        }, {
                                            "type": "NewEntityButton",
                                            "label": "Resource (Leaderway, CEP Game)",
                                            "targetClass": "ResourceDescriptor"
                                        }, {
                                            "type": "NewEntityButton",
                                            "label": "Dialogue (Leaderway)",
                                            "targetClass": "DialogueDescriptor"
                                        }, {
                                            "type": "NewEntityButton",
                                            "label": "Task (Leaderway)",
                                            "targetClass": "TaskDescriptor"
                                        }]
                                    }
                                }],
                                "targetClass": "VariableDescriptor"
                            }, {
                                "type": "Button",
                                "label": "<span class=\"wegas-icon wegas-icon-reset\"></span>Restart",
                                "plugins": [{
                                    "fn": "ResetAction"
                                }]
                            }]
                        }
                    }]
                }]
            },  {
                "type": "Tab",
                "label": "Players",
                "children": [{
                    "type": "EditorTreeView",
                    "dataSource": "Game"
                }]
            }]
        }]
    },
    "center": {
        "children": [{
            "id": "centerTabView",
            "type": "TabView",
            "children": [{
                "type": "Tab",
                "label": "Preview",
                "children": [{
                    "type": "PageLoader",
                    "pageLoaderId": "previewPageLoader",
                    "defaultPageId": 1,
                    "cssClass":"preview-content",
                    "plugins": [{
                        "fn": "WidgetToolbar",
                        "cfg": {
                            "children": []
                        }
                    }, {
                        "fn": "PageEditor"
                    }]
                }]
            }]
        }]
    },
    "right": {
        "width": 50,
        "resize": true,
        "collapse": false,
        "scroll": true,
        "animate": true,
        "children": [{
            "type": "TabView",
            "id": "rightTabView",
            "children": []
        }]
    }
}

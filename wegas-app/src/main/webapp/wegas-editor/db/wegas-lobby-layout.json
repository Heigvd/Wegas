{
    "id": 2003,
    "type" : "Layout",
    "top" : {
        "children": [{
                "type": "Text",
                "content": "Lobby",
                "cssClass": "wegas-title"
            }, {
            "type": "Button",
            "label": "Administrator",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "OpenTabButton",
                        "label": "Users",
                        "children": {
                            "type": "EditorTreeView",
                            "dataSource": "User",
                            "plugins": [{
                                "fn": "WidgetToolbar",
                                "cfg": {
                                    "children": [{
                                        "type": "NewEntityButton",
                                        "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                        "targetClass": "JpaAccount",
                                        "dataSource": "User"
                                    }]
                                }
                            }]
                        }
                    }, {
                        "type": "OpenTabButton",
                        "label": "Groups",
                        "children": {
                            "type": "EditorTreeView",
                            "dataSource": "Role",
                            "plugins": [{
                                "fn": "WidgetToolbar",
                                "cfg": {
                                    "children": [{
                                        "type": "NewEntityButton",
                                        "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                        "targetClass": "Role",
                                        "dataSource": "Role"
                                    }]
                                }
                            }]
                        }
                    }]
                }
            }]
    }, {
                "type": "LoginButton",
                "cssClass": "menu-loginbutton",
                "plugins": [{
                        "fn": "WidgetMenu",
                        "cfg": {
                            "children": [{
                                    "type": "Button",
                                    "label": "Preferences",
                                    "plugins": [{
                                            "fn": "OpenTabAction",
                                            "cfg": {
                                                "children": {
                                                    "type": "UserPreferences",
                                                    "content": "test12"
                                                }
                                            }
                                        }]
                                }, {
                                    "type": "Button",
                                    "label": "Logout",
                                    "plugins": [{
                                            "fn": "OpenUrlAction",
                                            "cfg": {
                                                "url": "wegas-app/logout",
                                                "target": "self"
                                            }
                                        }
                                    ]
                                }]
                        }
                    }]
            }]
    },
    "left": {
        "children": [{
                "type": "TabView",
                "id": "leftTabView",
                "children": [{
                        "type": "Tab",
                        "label": "Game models",
                        "children": [{
                                "type": "EditorTreeView",
                                "dataSource": "GameModel",
                                "plugins": [{
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "NewEntityButton",
                                                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New game model",
                                                    "targetClass": "GameModel",
                                                    "dataSource": "GameModel"
                                                }]
                                        }
                                    }]
                            }]
                    }]
            }]
    },
    "center": {
        "children": [{
                "id": "centerTabView",
                "type": "TabView",
                "children": [{
                        "id": "gamesTreeViewTab",
                        "type": "Tab",
                        "label": "Games",
                        "children": [{
                                "type": "EditorTreeView",
                                "dataSource": "Game",
                                "emptyMessage": "Click in the left column to select a game.",
                                "plugins": [{
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "NewEntityButton",
                                                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New game",
                                                    "targetClass": "Game",
                                                    "dataSource": "Game",
                                                    "disabled": true
                                                }]
                                        }
                                    }]
                            }]
                    }]
            }]
    },
    "right": {
        "width": 50,
        "resize": true,
        "collapse": false,
        "scroll": true,
        "animate": true,
        "children": [{
                "type": "TabView",
                "id": "rightTabView",
                "children": [],
                "plugins": [{
                        "fn": "LayoutToggleTab"
                    }]
            }]
    }
}

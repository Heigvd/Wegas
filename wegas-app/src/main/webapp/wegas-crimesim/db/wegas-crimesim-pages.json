{
    "1": {
        "id": 1,
        "direction": "vertical",
        "type": "List",
        "cssClass": "layout wegas-crimesim",
        "plugins": [{
                "fn": "BlockRightclick"
            }, {
                "fn": "CSSStyles",
                "cfg": {
                    "styles": {
                        "min-width": "700px"
                    }
                }
            }],
        "children": [{
                "type": "List",
                "cssClass": "header",
                "direction": "horizontal",
                "children": [{
                        "type": "Text",
                        "content": "Crimesim",
                        "cssClass": "wegas-crimesim-title"
                    }, {
                        "type": "ValueboxTemplate",
                        "data": {},
                        "variable": {
                            "content": "Variable.find('period')"
                        }
                    }, {
                        "disabled": false,
                        "type": "Button",
                        "label": "Période suivante",
                        "plugins": [{
                                "cfg": {
                                    "content": "Avancer à la période suivante"
                                },
                                "fn": "Tooltip"
                            }, {
                                "fn": "ExecuteScriptAction",
                                "cfg": {
                                    "targetEvent": "click",
                                    "onClick": {
                                        "@class": "Script",
                                        "language": "JavaScript",
                                        "content": "nextWeek();"
                                    }
                                }
                            }
                        ]
                    }, {
                        "type": "NumberTemplate",
                        "data": {},
                        "variable": {
                            "content": "Variable.find('cost')"
                        }
                    }, {
                        "type": "BoxTemplate",
                        "data": {
                            "maxValue": 15
                        },
                        "variable": {
                            "content": "Variable.find('humanResources')"
                        }
                    }
                ]
            }, {
                "id": "menu",
                "type": "List",
                "cssClass": "menu",
                "direction": "horizontal",
                "children": [
                    {
                        "type": "Button",
                        "label": "Accueil",
                        "disabled": false,
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "targetEvent": "click",
                                    "subpageId": 2,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }
                        ]
                    }, {
                        "type": "Button",
                        "disabled": false,
                        "label": "Questions",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "targetEvent": "click",
                                    "subpageId": 3,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }, {
                                "fn": "UnreadCount",
                                "cfg": {
                                    "variable": {
                                        "content": "Variable.find('questions')"
                                    }
                                }
                            }
                        ]
                    }, {
                        "type": "Button",
                        "disabled": false,
                        "label": "Pièces",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "targetEvent": "click",
                                    "subpageId": 4,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }
                        ]
                    }, {
                        "type": "Button",
                        "disabled": false,
                        "label": "Résultat des analyses",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "ChoicesRepliesUnreadCount",
                                "cfg": {
                                }
                            }, {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "targetEvent": "click",
                                    "subpageId": 6,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }
                        ]
                    }, {
                        "type": "Button",
                        "disabled": false,
                        "label": "Messages",
                        "cssClass": "menu-button",
                        "plugins": [
                            {
                                "fn": "OpenPageAction",
                                "cfg": {
                                    "targetEvent": "click",
                                    "subpageId": 5,
                                    "targetPageLoaderId": "maindisplayarea"
                                }
                            }, {
                                "fn": "UnreadCount",
                                "cfg": {
                                    "variable": {
                                        "content": "Variable.find('inbox')"
                                    }
                                }
                            }
                        ]
                    }, {
                        "cssClass": "menu-loginbutton",
                        "disabled": false,
                        "targetPageLoader": "maindisplayarea",
                        "type": "LoginButton"
                    }
                ]
            }, {
                "type": "PageLoader",
                "pageLoaderId": "maindisplayarea",
                "cssClass": "body",
                "defaultPageId": 2
            }
        ]
    },
    "2": {
        "type": "Template",
        "custom": "<%== this.value || 'Unable to find welcome text' %>",
        "data": {},
        "variable": {
            "content": "Variable.find('welcomeText')"
        }
    },
    "3": {
        "id": 3,
        "type": "MCQTabView",
        "variable": {
            "content": "Variable.find('questions')"
        }
    },
    "4": {
        "id": 4,
        "type": "ScheduleDisplay"
    },
    "5": {
        "id": 5,
        "type": "InboxDisplay",
        "variable": {
            "content": "Variable.find('inbox')"
        }
    },
    "6": {
        "id": 6,
        "type": "ResultsDisplay"
    }
}

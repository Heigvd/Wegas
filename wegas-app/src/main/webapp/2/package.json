    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "color": "^3.1.0",
    "downshift": "^3.2.6",
    "emotion": "^10.0.7",
    "fast-json-patch": "^2.0.7",
    "immer": "2.1.0",
    "immutable": "^3.8.2",
    "jsoninput": "github:cigit/jsoninput",
    "jsplumb": "^2.8.4",
    "lodash-es": "^4.17.11",
    "match-sorter": "^2.3.0",
    "monaco-editor": "^0.15.5",
    "pako": "^1.0.10",
    "pusher-js": "^4.3.1",
    "react": "^16.6.3",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.6.3",
    "react-testing-library": "^5.2.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.0",

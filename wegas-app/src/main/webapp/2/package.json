{
  "name": "wegas_ui2",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --mode production",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "lodash-es": "lodash"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": false
      }
    }
  },
  "dependencies": {
    "@babel/generator": "^7.0.0-beta.48",
    "@babel/traverse": "^7.0.0-beta.48",
    "@babel/types": "^7.0.0-beta.48",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "babylon": "^6.18.0",
    "downshift": "^1.31.14",
    "emotion": "^9.1.3",
    "fast-json-patch": "^2.0.6",
    "immer": "^1.3.0",
    "immutable": "^3.8.2",
    "jsoninput": "github:cigit/jsoninput",
    "jsplumb": "^2.6.9",
    "lodash-es": "^4.17.8",
    "match-sorter": "^2.2.3",
    "modern-normalize": "^0.4.0",
    "monaco-editor": "^0.13.1",
    "pako": "^1.0.6",
    "pusher-js": "^4.2.2",
    "react": "^16.4.0",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.4.0",
    "react-testing-library": "^3.1.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "slate": "^0.33.6",
    "slate-html-serializer": "^0.6.3",
    "slate-react": "^0.12.6"
  },
  "devDependencies": {
    "@types/babel-generator": "^6.25.1",
    "@types/babel-traverse": "^6.25.3",
    "@types/babylon": "^6.16.2",
    "@types/jest": "^23.0.0",
    "@types/lodash-es": "^4.17.0",
    "@types/pako": "^1.0.0",
    "@types/react": "^16.3.16",
    "@types/react-dnd": "^2.0.36",
    "@types/react-dnd-html5-backend": "^2.1.9",
    "@types/react-dom": "^16.0.5",
    "@types/react-grid-layout": "^0.16.4",
    "@types/slate": "git+https://github.com/JanLoebel/types-slate.git",
    "@types/slate-react": "git+https://github.com/JanLoebel/types-slate-react.git",
    "@types/webpack-env": "^1.13.3",
    "awesome-typescript-loader": "^5.0.0",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "fork-ts-checker-webpack-plugin": "^0.4.2",
    "jest": "^23.1.0",
    "monaco-editor-webpack-plugin": "^1.2.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.1",
    "typescript": "2.9.1",

{
  "name": "wegas_ui2",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --mode production",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    }
  },
  "dependencies": {
    "@babel/generator": "^7.0.0-beta.46",
    "@babel/traverse": "^7.0.0-beta.46",
    "@babel/types": "^7.0.0-beta.46",
    "@fortawesome/fontawesome": "^1.1.7",
    "@fortawesome/fontawesome-free-solid": "^5.0.12",
    "@fortawesome/react-fontawesome": "0.0.19",
    "babylon": "^6.18.0",
    "classnames": "^2.2.5",
    "downshift": "^1.31.14",
    "fast-json-patch": "^2.0.6",
    "glamor": "^2.20.40",
    "immer": "^1.3.0",
    "immutable": "^3.8.2",
    "jsoninput": "github:cigit/jsoninput",
    "lodash-es": "^4.17.8",
    "match-sorter": "^2.2.3",
    "modern-normalize": "^0.4.0",
    "monaco-editor": "^0.12.0",
    "pako": "^1.0.6",
    "pusher-js": "^4.2.2",
    "react": "^16.3.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-testing-library": "^3.0.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "slate": "^0.33.5",
    "slate-html-serializer": "^0.6.2",
    "slate-react": "^0.12.5"
  },
  "devDependencies": {
    "@types/babel-generator": "^6.25.1",
    "@types/babel-traverse": "^6.25.3",
    "@types/babylon": "^6.16.2",
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.2.3",
    "@types/lodash-es": "^4.17.0",
    "@types/pako": "^1.0.0",
    "@types/react": "^16.3.13",
    "@types/react-dnd": "^2.0.36",
    "@types/react-dnd-html5-backend": "^2.1.9",
    "@types/react-dom": "^16.0.5",
    "@types/react-grid-layout": "^0.16.4",
    "@types/react-redux": "^5.0.16",
    "@types/slate": "git+https://github.com/JanLoebel/types-slate.git",
    "@types/slate-react": "git+https://github.com/JanLoebel/types-slate-react.git",
    "@types/webpack-env": "^1.13.3",
    "autoprefixer": "^8.3.0",
    "awesome-typescript-loader": "^5.0.0",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.11",
    "fork-ts-checker-webpack-plugin": "^0.4.0",

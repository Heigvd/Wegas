{
  "name": "wegas_ui2",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --mode production",
    "test": "jest"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "lodash-es": "lodash"
    },
    "watchPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "babelConfig": false,
        "diagnostics": {
          "ignoreCodes": [
            151001
          ]
        }
      }
    }
  },
  "dependencies": {
    "@babel/generator": "^7.3.4",

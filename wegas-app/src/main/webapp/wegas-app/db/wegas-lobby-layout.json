{
    "id": 2003,
    "type": "Layout",
    "top": {
        "children": [{
                "type": "Text",
                "cssClass": "wegas-title"
            }, {
                "type": "Button",
                "label": "Administrator",
                "cssClass": "editor-administrator-button",
                "plugins": [{
                        "fn": "WidgetMenu",
                        "cfg": {
                            "children": [{
                                    "type": "OpenTabButton",
                                    "label": "Users",
                                    "wchildren": [{
                                            "type": "EditorTreeView",
                                            "dataSource": "User",
                                            "request": {
                                                "request": "/?view=Public"
                                            },
                                            "plugins": [{
                                                    "fn": "WidgetToolbar",
                                                    "cfg": {
                                                        "children": [{
                                                                "type": "NewEntityButton",
                                                                "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                                                "targetClass": "JpaAccount",
                                                                "dataSource": "User"
                                                            }]
                                                    }
                                                }]
                                        }]
                                }, {
                                    "type": "OpenTabButton",
                                    "label": "Groups",
                                    "wchildren": [{
                                            "type": "EditorTreeView",
                                            "dataSource": "Role",
                                            "request": {
                                                "request": "/"
                                            },
                                            "plugins": [{
                                                    "fn": "WidgetToolbar",
                                                    "cfg": {
                                                        "children": [{
                                                                "type": "NewEntityButton",
                                                                "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                                                "targetClass": "Role",
                                                                "dataSource": "Role"
                                                            }]
                                                    }
                                                }]
                                        }]
                                }]
                        }
                    }]
            }, {
                "type": "LoginButton",
                "cssClass": "menu-loginbutton",
                "plugins": [{
                        "fn": "WidgetMenu",
                        "cfg": {
                            "children": [{
                                    "type": "Button",
                                    "label": "Preferences",
                                    "plugins": [{
                                            "fn": "OpenTabAction",
                                            "cfg": {
                                                "wchildren": [{
                                                        "type": "Form",
                                                        "plugins": [{
                                                                "fn": "UserPreferences"
                                                            }]
                                                    }]
                                            }
                                        }]
                                }, {
                                    "type": "Button",
                                    "label": "Logout",
                                    "plugins": [{
                                            "fn": "OpenUrlAction",
                                            "cfg": {
                                                "url": "wegas-app/logout",
                                                "target": "self"
                                            }
                                        }]
                                }]
                        }
                    }]
            }]
    },
    "left": {
        "children": [{
                "type": "TabView",
                "id": "leftTabView",
                "cssClass": "editor-tab",
                "children": [{
                        "type": "Tab",
                        "label": "Models",
                        "children": [{
                                "type": "GameModelTreeView",
                                "dataSource": "GameModel",
                                "plugins": [{
                                        "fn": "EditorTVAdminMenu",
                                        "cfg": {
                                            "autoClick": false
                                        }
                                    }, {
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "NewEntityButton",
                                                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New model",
                                                    "targetClass": "GameModel",
                                                    "dataSource": "GameModel",
                                                    "formCfg": {
                                                        "type": "group",
                                                        "fields": [{
                                                                "name": "@class",
                                                                "type": "hidden"
                                                            }, {
                                                                "name": "templateId",
                                                                "type": "gamemodelselect",
                                                                "label": "Based on",
                                                                "filters": {
                                                                    "canDuplicate": true
                                                                }
                                                            }, {
                                                                "label": "name",
                                                                "name": "name",
                                                                "required": true
                                                            }, {
                                                                "label": "properties",
                                                                "name": "properties",
                                                                "useButtons": true,
                                                                "wrapperClassName": "inputEx-fieldWrapper wegas-advanced-feature",
                                                                "type": "object",
                                                                "fields": [],
                                                                "showMsg": true
                                                            }]
                                                    }
                                                }]
                                        }
                                    }]
                            }]
                    }]
            }]
    },
    "center": {
        "children": [{
                "id": "centerTabView",
                "type": "TabView",
                "cssClass": "editor-tab",
                "children": [{
                        "type": "Tab",
                        "id": "availableGamesTab",
                        "label": "Available games",
                        "children": [{
                                "type": "PublicGameTreeView",
                                "dataSource": "PublicGames",
                                "emptyMessage": "No new game is available to play.",
                                "plugins": [{
                                        "fn": "EditorTVAdminMenu",
                                        "cfg": {
                                            "children": [{
                                                    "type": "Button",
                                                    "label": "Select team",
                                                    "plugins": [{
                                                            "fn": "OpenTabAction",
                                                            "cfg": {
                                                                "tabSelector": "#rightTabView",
                                                                "wchildren": [{
                                                                        "type": "JoinTeam"
                                                                    }]
                                                            }
                                                        }]
                                                }]
                                        }
                                    }, {
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "Button",
                                                    "label": "<span class=\"wegas-icon wegas-icon-joingame\"></span>Join with token",
                                                    "plugins": [{
                                                            "fn": "OpenTabAction",
                                                            "cfg": {
                                                                "tabSelector": "#rightTabView",
                                                                "wchildren": [{
                                                                        "type": "JoinGame"
                                                                    }]
                                                            }
                                                        }]
                                                }, {
                                                    "type": "NewEntityButton",
                                                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>Host a game",
                                                    "targetClass": "Game",
                                                    "dataSource": "Game",
                                                    "formCfg": {
                                                        "required": true,
                                                        "type": "group",
                                                        "fields": [{
                                                                "name": "@class",
                                                                "type": "hidden",
                                                                "value": "null"
                                                            }, {
                                                                "label": "Model",
                                                                "name": "gameModelId",
                                                                "required": true,
                                                                "filters": {"canInstantiate": true},
                                                                "type": "gamemodelselect"
                                                            }, {
                                                                "label": "name",
                                                                "name": "name",
                                                                "required": true,
                                                                "showMsg": true
                                                            }, {
                                                                "label": "token",
                                                                "name": "token",
                                                                "description": "Leave blank for automatic generation"
                                                            }]
                                                    }
                                                }]
                                        }
                                    }]
                            }]
                    }, {
                        "type": "Tab",
                        "id": "joinedGamesTab",
                        "label": "Joined games",
                        "children": [{
                                "type": "JoinedGameTreeView",
                                "dataSource": "RegisteredGames",
                                "emptyMessage": "You have not joined any game yet.",
                                "plugins": [{
                                        "fn": "EditorTVAdminMenu",
                                        "cfg": {
                                            "children": [{
                                                    "type": "Button",
                                                    "plugins": [{
                                                            "fn": "OpenGameAction",
                                                            "cfg": {
                                                                "editorUrl": "wegas-app/view/play.html?"
                                                            }
                                                        }]
                                                }]
                                        }
                                    }, {
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "Button",
                                                    "label": "<span class=\"wegas-icon wegas-icon-joingame\"></span>Join with token",
                                                    "plugins": [{
                                                            "fn": "OpenTabAction",
                                                            "cfg": {
                                                                "tabSelector": "#rightTabView",
                                                                "wchildren": [{
                                                                        "type": "JoinGame"
                                                                    }]
                                                            }
                                                        }]
                                                }, {
                                                    "type": "NewEntityButton",
                                                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>Host a game",
                                                    "targetClass": "Game",
                                                    "dataSource": "Game",
                                                    "formCfg": {
                                                        "required": true,
                                                        "type": "group",
                                                        "fields": [{
                                                                "name": "@class",
                                                                "type": "hidden",
                                                                "value": "null"
                                                            }, {
                                                                "label": "Model",
                                                                "name": "gameModelId",
                                                                "required": true,
                                                                "filters": {"canInstantiate": true},
                                                                "type": "gamemodelselect"
                                                            }, {
                                                                "label": "name",
                                                                "name": "name",
                                                                "required": true,
                                                                "showMsg": true
                                                            }, {
                                                                "label": "token",
                                                                "name": "token",
                                                                "description": "Leave blank for automatic generation"
                                                            }]
                                                    }
                                                }]
                                        }
                                    }]
                            }]
                    }, {
                        "id": "gamesTreeViewTab",
                        "type": "Tab",
                        "label": "Hosted games",
                        "children": [{
                                "type": "CreatedGameTreeView",
                                "dataSource": "Game",
                                "emptyMessage": "You have not created any game yet",
                                "plugins": [{
                                        "fn": "EditorTVAdminMenu",
                                        "cfg": {
                                            "autoClick": false
                                        }
                                    }, {
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "Button",
                                                    "label": "<span class=\"wegas-icon wegas-icon-joingame\"></span>Join with token",
                                                    "plugins": [{
                                                            "fn": "OpenTabAction",
                                                            "cfg": {
                                                                "tabSelector": "#rightTabView",
                                                                "wchildren": [{
                                                                        "type": "JoinGame"
                                                                    }]
                                                            }
                                                        }]
                                                }, {
                                                    "type": "NewEntityButton",
                                                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>Host a game",
                                                    "targetClass": "Game",
                                                    "dataSource": "Game",
                                                    "formCfg": {
                                                        "required": true,
                                                        "type": "group",
                                                        "fields": [{
                                                                "name": "@class",
                                                                "type": "hidden",
                                                                "value": "null"
                                                            }, {
                                                                "label": "Models",
                                                                "name": "gameModelId",
                                                                "required": true,
                                                                "filters": {"canInstantiate": true},
                                                                "type": "gamemodelselect"
                                                            }, {
                                                                "label": "name",
                                                                "name": "name",
                                                                "required": true,
                                                                "showMsg": true
                                                            }, {
                                                                "label": "token",
                                                                "name": "token",
                                                                "description": "Leave blank for automatic generation"
                                                            }]
                                                    }
                                                }]
                                        }
                                    }]
                            }]
                    }]
            }]
    },
    "right": {
        "width": 50,
        "resize": true,
        "collapse": false,
        "scroll": true,
        "animate": true,
        "children": [{
                "type": "TabView",
                "id": "rightTabView",
                "cssClass": "editor-tab",
                "children": [],
                "plugins": [{
                        "fn": "LayoutToggleTab"
                    }]
            }]
    }
}

{
    "id": 2003,
    "type": "ResizableLayout",
    "top": {
        "children": [{
                "type": "Text",
                "cssClass": "wegas-title"
            }, {
                "type": "Button",
                "label": "Admin",
                "cssClass": "editor-administrator-button",
                "plugins": [{
                        "fn": "WidgetMenu",
                        "cfg": {
                            "children": [{
                                    "type": "OpenTabButton",
                                    "label": "Users",
                                    "wchildren": [{
                                            "type": "EditorTreeView",
                                            "dataSource": "User",
                                            "request": {
                                                "request": "/?view=Public"
                                            },
                                            "plugins": [{
                                                    "fn": "WidgetToolbar",
                                                    "cfg": {
                                                        "children": [{
                                                                "type": "NewEntityButton",
                                                                "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                                                "targetClass": "JpaAccount",
                                                                "dataSource": "User"
                                                            }]
                                                    }
                                                }, {
                                                    "fn": "EditorTVDefaultMenuClick"
                                                }]
                                        }]
                                }, {
                                    "type": "OpenTabButton",
                                    "label": "Groups",
                                    "wchildren": [{
                                            "type": "EditorTreeView",
                                            "dataSource": "Role",
                                            "request": {
                                                "request": "/"
                                            },
                                            "plugins": [{
                                                    "fn": "WidgetToolbar",
                                                    "cfg": {
                                                        "children": [{
                                                                "type": "NewEntityButton",
                                                                "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                                                                "targetClass": "Role",
                                                                "dataSource": "Role"
                                                            }]
                                                    }
                                                }, {
                                                    "fn": "EditorTVDefaultMenuClick"
                                                }]
                                        }]
                                }]
                        }
                    }]
            }, {
                "type": "LoginButton",
                "cssClass": "menu-loginbutton",
                "plugins": [{
                        "fn": "WidgetMenu",
                        "cfg": {
                            "children": [{
                                    "type": "Button",
                                    "label": "Preferences",
                                    "plugins": [{
                                            "fn": "OpenTabAction",
                                            "cfg": {
                                                "wchildren": [{
                                                        "type": "Form",
                                                        "plugins": [{
                                                                "fn": "UserPreferences"
                                                            }]
                                                    }]
                                            }
                                        }]
                                }, {
                                    "type": "Button",
                                    "label": "Logout",
                                    "plugins": [{
                                            "fn": "OpenUrlAction",
                                            "cfg": {
                                                "url": "logout",
                                                "target": "self"
                                            }
                                        }]
                                }]
                        }
                    }]
            }]
    },
    "center": {
        "width": 840,
        "children": [{
                "id": "centerTabView",
                "type": "TabView",
                "cssClass": "wegas-tabview-fullheight",
                "children": [{
                        "type": "Tab",
                        "id": "joinedGamesTab",
                        "label": "Player",
                        "children": [{
                                "type": "GameDataTable",
                                "dataSource": "RegisteredGames",
                                "cssClass": "wegas-joinedgamesdatatable",
                                "emptyMessage": "You have not joined any game yet",
                                "dataTableCfg": {
                                    "columns": [{
                                            "key": "iconCSS",
                                            "formatter": "icon",
                                            "label": " ",
                                            "sortable": false,
                                            "width": "27px"
                                        }, {
                                            "key": "name",
                                            "label": "Name",
                                            "formatter": "link"
                                        }, {
                                            "key": "gameModelName",
                                            "label": "Scenario",
                                            "width": "150px"
                                        }, {
                                            "key": "createdBy",
                                            "label": "Created by",
                                            "width": "150px"
                                        }, {
                                            "key": "createdTime",
                                            "label": "Joined",
                                            "width": "125px",
                                            "formatter": "date"
                                        }]
                                },
                                "plugins": [{
                                        "fn": "EditorDTMenu",
                                        "cfg": {
                                            "childrenf": [{
                                                    "type": "Button",
                                                    "plugins": [{
                                                            "fn": "OpenGameAction",
                                                            "cfg": {
                                                                "editorUrl": "play.html?"
                                                            }
                                                        }]
                                                }],
                                            "children": [{
                                                    "type": "Button",
                                                    "plugins": [{
                                                            "fn": "OpenTabAction",
                                                            "cfg": {
                                                                "tabSelector": "#rightTabView",
                                                                "emptyTab": true,
                                                                "label": "Details",
                                                                "wchildren": [{
                                                                        "type": "List",
                                                                        "children": [{
                                                                                "type": "GameInformation",
                                                                                "cssClass": "open-game-tab"
                                                                            }, {
                                                                                "type": "Button",
                                                                                "cssClass": "editor-button-resume",
                                                                                "label": "Open game",
                                                                                "plugins": [{
                                                                                        "fn": "OpenGameAction",
                                                                                        "cfg": {
                                                                                            "editorUrl": "play.html?"
                                                                                        }
                                                                                    }]
                                                                            }]
                                                                    }]
                                                            }
                                                        }]
                                                }]
                                        }
                                    }, {
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "Button",
                                                    "label": "<span class=\"wegas-icon wegas-icon-joingame\"></span>Join game",
                                                    "plugins": [{
                                                            "fn": "WidgetMenu",
                                                            "cfg": {
                                                                "children": [{
                                                                        "type": "Button",
                                                                        "label": "Join with an enrolment key",
                                                                        "plugins": [{
                                                                                "fn": "OpenTabAction",
                                                                                "cfg": {
                                                                                    "tabSelector": "#rightTabView",
                                                                                    "emptyTab": true,
                                                                                    "wchildren": [{
                                                                                            "type": "TokenJoin"
                                                                                        }]
                                                                                }
                                                                            }]
                                                                    }, {
                                                                        "type": "Button",
                                                                        "label": "Join a public game",
                                                                        "plugins": [{
                                                                                "fn": "OpenTabAction",
                                                                                "cfg": {
                                                                                    "label": "Public games",
                                                                                    "tabSelector": "#rightTabView",
                                                                                    "emptyTab": true,
                                                                                    "wchildren": [{
                                                                                            "type": "PublicGameDataTable"
                                                                                        }]
                                                                                }
                                                                            }]
                                                                    }]
                                                            }
                                                        }]
                                                }]
                                        }
                                    }]
                            }]
                    }, {
                        "id": "gamesTreeViewTab",
                        "type": "Tab",
                        "label": "Trainer",
                        "children": [{
                                "type": "GameDataTable",
                                "dataSource": "Game",
                                "cssClass": "wegas-hostedgamesdatatable",
                                "emptyMessage": "You have not created any game yet",
                                "plugins": [{
                                        "fn": "EditorDTMouseOverMenu"
                                    }, {
                                        "fn": "EditorDTMenu"
                                    }, {
                                        "fn": "EditorDTContextMenu"
                                    }, {
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "NewEntityButton",
                                                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New game",
                                                    "targetClass": "Game",
                                                    "dataSource": "Game",
                                                    "formCfg": {
                                                        "required": true,
                                                        "type": "group",
                                                        "fields": [{
                                                                "name": "@class",
                                                                "type": "hidden",
                                                                "value": "null"
                                                            }, {
                                                                "label": "name",
                                                                "name": "name",
                                                                "required": true,
                                                                "showMsg": true
                                                            }, {
                                                                "label": "Scenario",
                                                                "name": "gameModelId",
                                                                "required": true,
                                                                "filters": {
                                                                    "canInstantiate": true
                                                                },
                                                                "type": "gamemodelselect"
                                                            }]
                                                    }
                                                }]
                                        }
                                    }]
                            }]
                    }, {
                        "type": "Tab",
                        "id": "gameModelsTab",
                        "label": "Scenarist",
                        "children": [{
                                "type": "GameDataTable",
                                "dataSource": "GameModel",
                                "cssClass": "wegas-gamemodeldatatable",
                                "emptyMessage": "You have not created any scenario yet",
                                "dataTableCfg": {
                                    "columns": [{
                                            "key": "iconCSS",
                                            "formatter": "icon",
                                            "label": " ",
                                            "sortable": false,
                                            "width": "27px"
                                        }, {
                                            "key": "name",
                                            "label": "Name",
                                            "formatter": "link"
                                        }, {
                                            "key": "createdBy",
                                            "label": "Created by",
                                            "width": "150px"
                                        }, {
                                            "key": "createdTime",
                                            "label": "Created",
                                            "width": "125px",
                                            "formatter": "date"
                                        }
                                    ]
                                },
                                "plugins": [{
                                        "fn": "EditorDTMenu"
                                    }, {
                                        "fn": "EditorDTContextMenu"
                                    }, {
                                        "fn": "EditorDTMouseOverMenu"
                                    }, {
                                        "fn": "WidgetToolbar",
                                        "cfg": {
                                            "children": [{
                                                    "type": "NewEntityButton",
                                                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New scenario",
                                                    "targetClass": "GameModel",
                                                    "dataSource": "GameModel",
                                                    "formCfg": {
                                                        "type": "group",
                                                        "fields": [{
                                                                "name": "@class",
                                                                "type": "hidden"
                                                            }, {
                                                                "name": "templateId",
                                                                "type": "gamemodelselect",
                                                                "label": "Based on",
                                                                "filters": {
                                                                    "canDuplicate": true
                                                                }
                                                            }, {
                                                                "label": "name",
                                                                "name": "name",
                                                                "required": true
                                                            }, {
                                                                "label": "properties",
                                                                "name": "properties",
                                                                "wrapperClassName": "inputEx-fieldWrapper wegas-advanced-feature",
                                                                "type": "object",
                                                                "fields": [],
                                                                "showMsg": true
                                                            }]
                                                    }
                                                }]
                                        }
                                    }]
                            }]
                    }]
            }]
    },
    "right": {
        "children": [{
                "type": "List",
                "children": [{
                        "type": "TabView",
                        "id": "rightTabView",
                        "cssClass": "wegas-tabview-fullheight"
                    }],
                "plugins": [{
                        "fn": "PopupListener",
                        "cfg": {
                            "filter": ["success"]
                        }
                    }, {
                        "fn": "Injector"
                    }]
            }]
    }
}

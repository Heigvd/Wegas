{
    "1": {
        "id": 1,
        "label": "Main layout",
        "type": "List",
        "cssClass": "layout",
        "children": [{
                "type": "List",
                "cssClass": "header",
                "children": [{
                        "type": "List",
                        "direction": "horizontal",
                        "cssClass": "variables",
                        "children": [{
                                "type": "Text",
                                "content": "",
                                "cssClass": "title"
                            }, {
                                "type": "List",
                                "cssClass": "timeIndicators",
                                "children": [{
                                        "type": "VariableDisplay",
                                        "label": "Phase",
                                        "view": "title",
                                        "variable": {
                                            "name": "phases"
                                        }
                                    }, {
                                        "type": "VariableDisplay",
                                        "label": "Period",
                                        "view": "valuebox",
                                        "variable": {
                                            "name": "phases"
                                        }
                                    }]
                            }, {
                                "type": "Button",
                                "label": "Next period",
                                "view": "button",
                                "tooltip": "Advance to next period",
                                "plugins": [{
                                        "fn": "ExecuteScriptAction",
                                        "cfg": {
                                            "onClick": {
                                                "@class": "Script",
                                                "language": "JavaScript",
                                                "content": "passPeriod();"
                                            }
                                        }
                                    }],
                                "cssClass": "fl"
                            }, {
                                "type": "VariableDisplay",
                                "label": "Time cards",
                                "view": "box",
                                "variable": {
                                    "name": "humanResources"
                                },
                                "cssClass": "fr timecards"
                            }, {
                                "type": "GaugeDisplay",
                                "variable": {
                                    "name": "customerApproval"
                                },
                                "cssClass": "fr"
                            }
                        ]
                    }, {
                        "type": "List",
                        "cssClass": "menu",
                        "direction": "horizontal",
                        "children": [{
                                "type": "Button",
                                "label": "<span class=\"wegas-icon\"></span>Accueil",
                                "cssClass": "menu-button",
                                "plugins": [
                                    {
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "subpageId": 2,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }
                                ]
                            }, {
                                "type": "Button",
                                "label": "<span class=\"wegas-icon\"></span>Questions",
                                "cssClass": "menu-button",
                                "plugins": [
                                    {
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "subpageId": 4,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }, {
                                        "fn": "UnreadCount",
                                        "cfg": {
                                            "variable": {
                                                "name": "questions2"
                                            }
                                        }
                                    }
                                ]
                            }, {
                                "type": "Button",
                                "label": "<span class=\"wegas-icon\"></span>Mails",
                                "cssClass": "menu-button",
                                "plugins": [
                                    {
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "subpageId": 6,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }, {
                                        "fn": "UnreadCount",
                                        "cfg": {
                                            "variable": {
                                                "name": "inbox"
                                            }
                                        }
                                    }
                                ]
                            }, {
                                "type": "LoginButton",
                                "cssClass": "menu-loginbutton"
                            }
                        ]
                    }
                ]
            }, {
                "type": "PageLoader",
                "pageLoaderId": "maindisplayarea",
                "cssClass": "body",
                "defaultPageId": 2
            }, {
                "type": "List",
                "cssClass": "footer",
                "direction": "horizontal",
                "children": [
                    {
                        "type": "Text",
                        "content": "Copyright 2012 Heig-vd.ch"
                    }
                ]
            }
        ]
    },
    "2": {
        "id": 2,
        "type": "Text",
        "content": "Welcome to Wegas"
    },
    "4": {
        "id": 4,
        "type": "MCQTabView",
        "variable": {
            "expr": "questions.descriptorId"
        }
    },
    "6": {
        "id": 6,
        "type": "InboxDisplay",
        "variable": {
            "name": "inbox"
        }
    },
    "1000": {
        "id": 1000,
        "type": "Form",
        "plugins": [{
            "fn": "UserPreferences"
        }]
    }
}
{
    "1": {
        "id": 1,
        "type": "List",
        "cssClass": "layout",
        "direction": "vertical",
        "children": [{
                "type": "List",
                "cssClass": "header",
                "direction": "vertical",
                "children": [{
                        "type": "List",
                        "cssClass": "variables",
                        "direction": "horizontal",
                        "children": [{
                                "type": "Text",
                                "cssClass": "title",
                                "content": ""
                            }, {
                                "type": "List",
                                "cssClass": "timeIndicators",
                                "direction": "vertical",
                                "children": [{
                                        "type": "Template",
                                        "variable": {
                                            "name": "phases"
                                        },
                                        "template": "title"
                                    }, {
                                        "type": "Template",
                                        "variable": {
                                            "name": "phases"
                                        },
                                        "template": "valuebox"
                                    }]
                            }, {
                                "disabled": false,
                                "label": "Next period",
                                "type": "Button",
                                "cssClass": "fl",
                                "plugins": [{
                                        "fn": "Tooltip",
                                        "cfg": {
                                            "content": "Advance to next period"
                                        }
                                    }, {
                                        "fn": "ExecuteScriptAction",
                                        "cfg": {
                                            "targetEvent": "click",
                                            "onClick": {
                                                "@class": "Script",
                                                "language": "JavaScript",
                                                "content": "passPeriod();"
                                            }
                                        }
                                    }]
                            }, {
                                "type": "Template",
                                "cssClass": "fr timecards",
                                "variable": {
                                    "name": "humanResources"
                                },
                                "template": "box",
                                "data": {
                                    "label": "Time cards"
                                }
                            }, {
                                "type": "GaugeDisplay",
                                "cssClass": "fr",
                                "variable": {
                                    "name": "customerApproval"
                                }
                            }
                        ]
                    }, {
                        "type": "List",
                        "cssClass": "menu",
                        "direction": "horizontal",
                        "children": [{
                                "disabled": false,
                                "type": "Button",
                                "label": "<span class=\"wegas-icon\"></span>Accueil",
                                "cssClass": "menu-button",
                                "plugins": [
                                    {
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "targetEvent": "click",
                                            "subpageId": 2,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }
                                ]
                            }, {
                                "disabled": false,
                                "label": "<span class=\"wegas-icon\"></span>Questions",
                                "type": "Button",
                                "cssClass": "menu-button",
                                "plugins": [
                                    {
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "targetEvent": "click",
                                            "subpageId": 4,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }, {
                                        "fn": "UnreadCount",
                                        "cfg": {
                                            "variable": {
                                                "name": "questions"
                                            }
                                        }
                                    }
                                ]
                            }, {
                                "disabled": false,
                                "label": "<span class=\"wegas-icon\"></span>Mails",
                                "type": "Button",
                                "cssClass": "menu-button",
                                "plugins": [
                                    {
                                        "fn": "OpenPageAction",
                                        "cfg": {
                                            "targetEvent": "click",
                                            "subpageId": 6,
                                            "targetPageLoaderId": "maindisplayarea"
                                        }
                                    }, {
                                        "fn": "UnreadCount",
                                        "cfg": {
                                            "variable": {
                                                "name": "inbox"
                                            }
                                        }
                                    }
                                ]
                            }, {
                                "cssClass": "menu-loginbutton",
                                "disabled": false,
                                "label": "",
                                "labelIsUser": false,
                                "preferencePageId": 1000,
                                "targetPageLoader": "maindisplayarea",
                                "type": "LoginButton"
                            }
                        ]
                    }
                ]
            }, {
                "type": "PageLoader",
                "pageLoaderId": "maindisplayarea",
                "cssClass": "body",
                "defaultPageId": 2
            }, {
                "type": "List",
                "cssClass": "footer",
                "direction": "horizontal",
                "children": [
                    {
                        "type": "Text",
                        "content": "Copyright 2012 Heig-vd.ch"
                    }
                ]
            }
        ]
    },
    "2": {
        "id": 2,
        "type": "Text",
        "content": "Welcome to Wegas"
    },
    "4": {
        "id": 4,
        "type": "MCQTabView",
        "variable": {
            "expr": "questions.descriptorId"
        }
    },
    "6": {
        "id": 6,
        "type": "InboxDisplay",
        "variable": {
            "name": "inbox"
        }
    },
    "1000": {
        "id": 1000,
        "type": "Form",
        "plugins": [{
                "fn": "UserPreferences"
            }]
    }
}
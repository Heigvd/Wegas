{
    "id": 2003,
    "type" : "Layout",
    "top" : {
        "height": 25,
        "scroll": null,
        "zIndex": 2,
        "children": [{
            "type": "Button",
            "label": "Developer",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "OpenTabButton",
                        "label": "Console",
                        "subpage": {
                            "type": "Console",
                            "class": "wegas-console"
                        },
                        "toolbarChildren": [{
                            "type": "SelectPlayer"
                        }]
                    }, {
                        "type": "OpenTabButton",
                        "label": "Wysiwyg script editor",
                        "subpage": {
                            "type": "WysiwygEditor"
                        }
                    }, {
                        "type": "OpenTabButton",
                        "label": "Pages",
                        "subpage": {
                            "type": "EditorTreeView",
                            "dataSource": "Page"
                        }
                    }, {
                        "type": "OpenTabButton",
                        "label": "Log",
                        "subpage": {
                            "type": "Logger"
                        }
                    }]
                }
            }]
        },{
            "type": "Button",
            "label": "Scenarist",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "OpenTabButton",
                        "label": "Styles",
                        "subpage": {
                            "type": "CSSEditor",
                            "class": "wegas-csseditor"
                        }
                    },{
                        "type": "OpenTabButton",
                        "label": "Files",
                        "subpage": {
                            "type": "FileExplorer"
                        }
                    },{
                        "type": "OpenTabButton",
                        "label": "Scripts",
                        "subpage": {
                            "type": "ScriptLibrary"
                        }
                    }]
                }
            }]
        },{
            "type": "Button",
            "label": "Animator",
            "plugins": [{
                "fn": "WidgetMenu",
                "cfg": {
                    "children": [{
                        "type": "OpenTabButton",
                        "label": "Players",
                        "subpage": {
                            "type": "DataTable",
                            "dataSource": "User",
                            "columnset": [ {
                                "key":"id",
                                "sortable": true
                            }, {
                                "key":"name",
                                "sortable": true
                            }, {
                                "key":"@class"
                            }]
                        }
                    }]
                }
            }]
        }]
    },
    "left": {
        "width": 300,
        "resize": true,
        "scroll": true,
        "animate": true,
        "children": [{
            "type": "TabView",
            "children": [{
                "type": "Tab",
                "label": "Variables",
                "toolbarChildren": [{
                    "type": "NewEntityButton",
                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                    "targetClass": "VariableDescriptor"
                }, {
                    "type": "Button",
                    "label": "<span class=\"wegas-icon wegas-icon-reset\"></span>Restart",
                    "plugins": [{
                        "fn": "ResetAction"
                    }]
                }],
                "children": [
                {
                    "type": "EditorTreeView",
                    "dataSource": "VariableDescriptor",
                    "excludeClasses": {
                        "MCQDescriptor": true
                    }
                }]
            }]
        }]
    },
    "center": {
        "children": [{
            "id": "centerTabView",
            "type": "TabView",
            "children": [{
                "type": "Tab",
                "label": "Preview",
                "toolbarChildren": [{
                    "type": "SelectPlayer"
                }],
                "children": [{
                    "type": "PageLoader",
                    "pageId": 1
                }]
            }, {
                "type": "Tab",
                "label": "Games",
                "children": [{
                    "type": "EditorTreeView",
                    "dataSource": "Game"
                }],
                "toolbarChildren": [{
                    "type": "NewEntityButton",
                    "label": "<span class=\"wegas-icon wegas-icon-new\"></span>New",
                    "targetClass": "Game"
                }]
            }]
        }]
    },
    "right": {
        "width": 50,
        "resize": true,
        "collapse": false,
        "scroll": true,
        "animate": true,
        "children": [{
            "type": "TabView",
            "id": "rightTabView",
            "children": [
            {
                "type": "Tab",
                "label": "Chat",
                "toolbarLabel": "Chat",
                "children": [{
                    "type": "Chat"
                }]
            }]
        }]
    }
}

{
    "type" : "Layout",
    "right": {
        "width": 50,
        "resize": true,
        "collapse": false,
        "scroll": true,
        "animate": true,
        "children": [{
            "type": "TabView",
            "id": "rightTabView",
            "children": [
            {
                "type": "Tab",
                "label": "Console",
                "toolbarButtons": [
                {
                    "type": "select",
                    "label": "Francois",
                    "lazyloadmenu": false,
                    "value": "selectplayer",
                    "menu": [{
                        "text": "notloaded",
                        "value": "null"
                    }]
                }],
                "children": [
                {
                    "type": "Console",
                    "class": "wegas-console"
                }
                ]
            },
            {
                "type": "Tab",
                "label": "Chat",
                "toolbarLabel": "Chat",
                "children": [
                {
                    "type": "Chat"
                }
                ]
            }, {
                "type": "Tab",
                "label": "CSS",
                "toolbarLabel": "Edit CSS",
                "children": [
                {
                    "type": "CSSEditor",
                    "class": "wegas-csseditor"
                }
                ]
            }, {
                "type": "Tab",
                "label": "Log",
                "toolbarLabel": "Log",
                "children": [
                {
                    "type": "Logger"
                }
                ]
            }]
        }]
    },
    "top" : {
        "height": 25,
        "scroll": null,
        "zIndex": 2,
        "children": [{
            "type": "EditorTopMenu"
        }]
    },
    "left": {
        "width": 300,
        "resize": true,
        "scroll": true,
        "animate": true,
        "children": [{
            "type": "TabView",
            "children": [
            {
                "type": "Tab",
                "label": "Variables",
                "children": [
                {
                    "type": "WTreeView",
                    "dataSource": "VariableDescriptor",
                    "excludeClasses": {
                        "MCQDescriptor": true
                    }
                }],
                "toolbarButtons": [{
                    "type": "push",
                    "label": "New variable",
                    "value": "new",
                    "id": "new",
                    "data": {
                        "dataSource":"VariableDescriptor",
                        "@class": "VariableDescriptor"
                    }
                },{
                    "type": "push",
                    "label": "Reset this game",
                    "value": "reset",
                    "id": "reset"
                }]
            }, {
                "type": "Tab",
                "label": "Pages",
                "children": [{
                    "type": "WTreeView",
                    "dataSource": "Page"
                }],
                "toolbarButtons": []
            },
            {
                "type": "Tab",
                "label": "Users",
                "children": [
                {
                    "type": "DataTable",
                    "dataSource": "User",
                    "columnset": [ {
                        "key":"id",
                        "sortable": true
                    }, {
                        "key":"name",
                        "sortable": true
                    }, {
                        "key":"@class"
                    }]
                }
                ],
                "toolbarButtons": []
            }
            ]
        }]
    },
    "center": {
        "children": [{
            "id": "centerTabView",
            "type": "TabView",
            "children": [
            {
                "type": "Tab",
                "label": "Preview",
                "toolbarButtons": [
                {
                    "type": "select",
                    "label": "Francois",
                    "lazyloadmenu": false,
                    "value": "selectplayer",
                    "menu": [{
                        "text": "notloaded",
                        "value": "null"
                    }]
                }
                ],
                "children": [{
                    "type": "WidgetLoader",
                    "pageId": 1
                }]
            }, {
                "type": "Tab",
                "label": "Games",
                "children": [{
                    "type": "WTreeView",
                    "dataSource": "Game"
                }],
                "toolbarButtons": [{
                    "type": "push",
                    "label": "New game",
                    "value": "new",
                    "id": "new",
                    "data": {
                        "dataSource":"Game",
                        "@class": "Game"
                    }
                }]
            }, {
                "type": "Tab",
                "label": "File explorer",
                "children": [{
                    "type": "FileExplorer"
                }]
            }]
        }]
    }
}